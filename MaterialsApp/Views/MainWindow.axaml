<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:vm="using:MaterialsApp.ViewModels"
        xmlns:root="using:MaterialsApp"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:rui="using:ReactiveUI.Avalonia"
        mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
        x:Class="MaterialsApp.Views.MainWindow"
        x:DataType="vm:MainWindowViewModel"
        Icon="/Assets/avalonia-logo.ico"
        Title="MaterialsApp"
        MinHeight="900"
        MinWidth="900">
        <rui:RoutedViewHost Router="{Binding Router}">
                <rui:RoutedViewHost.ViewLocator><root:ViewLocatorVM></root:ViewLocatorVM></rui:RoutedViewHost.ViewLocator>
        </rui:RoutedViewHost>
</Window>
